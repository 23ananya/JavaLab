import java.util.*;
public class Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n;
		int[] num= new int[10];
		System.out.println("Enter the arrayy elements: ");
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		for(int i=0;i<n;i++) {
			num[i]=sc.nextInt();
		}
		for(i=0;i<n;i++) {
		System.out.println(num[i]);

	}

}
